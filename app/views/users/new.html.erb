
<h1>Sign Up</h1>
<%= form_for @user do |f|%>
  <%= f.label :username%><br>
  <%= f.text_field :username%><br>
  <%= f.label :password%><br>
  <%= f.password_field :password%><br>
  <%= f.label "Profile Photo"%>
  <%= f.file_field :avatar %><br>
  <%= f.submit %>
<%end%>
<%= button_to "Main Menu", welcome_path, method: :get%>
